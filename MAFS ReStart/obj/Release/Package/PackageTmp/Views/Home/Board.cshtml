                   @{
                       var counter = 2;
                   }
                   @foreach (var member in Model)
                   {
                       <div class="col-sm-6 col-md-3">
                             <div class="team_member">
                                  <figure style="background-image: url(../../../images/board/@member.Image2)"><img src="~/images/board/@member.Image" alt="@member.FirstName @member.LastName"></figure>
                                  <h5>@member.FirstName @member.LastName</h5>
                                  <small>@member.Position</small>
                                  <hr>
                                 <div class="team_social">
                                     @if (member.Facebook != string.Empty)
                                     {
                                         <a href="@member.Facebook" target="_blank"><i class="fa fa-facebook"></i></a>
                                     }
                                     @if (member.LinkedIn != string.Empty)
                                     {
                                         <a href="@member.LinkedIn" target="_blank"><i class="fa fa-linkedin"></i></a>
                                     }
                                     @if (member.Email != string.Empty)
                                     {
                                         <a href="mailto: @member.Email" target="_blank"><i class="fa fa-envelope"></i></a>
                                     }
                                 </div>
                                  @*<p class="short_bio">@member.FirstName @member.LastName</p>*@
                                 <br />
                                 @*<a class="btn btn-primary" href="#">Full Bio &raquo;</a>*@
                             </div>
                        </div>
                       
                       if(counter%4 == 0)
                       {
                            @:</div><br /><div class="row">
                       }
                       counter++;
                   }
                   </div>
            
