@model IEnumerable<MAFS_ReStart.Models.Program>

<li class="dropdown">
    <a data-toggle="dropdown" class="dropdown-toggle" href="#">Programs<span class="caret"></span></a>
    <ul class="dropdown-menu">
        <li>@Html.ActionLink("Overview" , "Index", "Program")</li>

        @foreach(var program in Model){
            <li>@Html.ActionLink( program.Title.Replace("-", " "), "Info", "Program", new { id = program.Title }, null)</li>
        }
    </ul>
</li>