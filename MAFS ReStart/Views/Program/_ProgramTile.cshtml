@model IEnumerable<MAFS_ReStart.Models.Program>

@foreach(var program in Model)
{
    <div class="col-sm-4 col-md-4">
        <div class="service_teaser vertical">
            <div class="service_photo">
                <figure style="background-image: url(../Images/@program.BannerImg)"></figure>
            </div>
            <div class="service_details">
                <h2 class="section_header skincolored"><strong>@program.Title.Replace("-", " ")</strong></h2>
                <p>
                    @program.Description.Substring(0,150) ...
                </p>
                <a class="btn btn-success" href="~/Program/Info/@program.Title"><i class="fa fa-info"></i> Learn More</a>
            </div>
        </div>

    </div>
}